public static int camelcase(String s) {
    // Write your code here
     int count=0;
     
     for(int i=0;i<s.length();i++)
     {
         char ch= s.charAt(i);
        
          if(ch>=65 && ch<=90)
          {
              count++;
              
          }
          
          
     }
     return count+1;
    }

}
